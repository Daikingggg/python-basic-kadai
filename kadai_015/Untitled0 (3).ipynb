        "class Human:\n",
        "\n",
        "    def __init__(self, name, age):\n",
        "        self.name = name\n",
        "        self.age = age\n",
        "    def printInfo(self):\n",
        "        print('私の名前は',self.name,'です')\n",
        "        print('私は',self.age,'才です')\n",
        "\n",
